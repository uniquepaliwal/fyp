<style>
  body {
    background: linear-gradient(135deg, #98D8EF, #E8F9FF);
    overflow-x: hidden;
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  }

  @keyframes waves {
    0% {
      transform: translateX(0) translateY(0);
    }

    50% {
      transform: translateX(-25%) translateY(5px);
    }

    100% {
      transform: translateX(-50%) translateY(0);
    }
  }

  .waves {
    position: absolute;
    width: 200%;
    height: 120px;
    bottom: 0;
    left: 0;
    background: url('https://i.imgur.com/5MZH8bd.png') repeat-x;
    animation: waves 12s cubic-bezier(0.44, 0.01, 0.55, 0.94) infinite;
    opacity: 0.8;
    z-index: 0;
  }


  @keyframes fadeInUp {
    from {
      opacity: 0;
      transform: translateY(20px);
    }

    to {
      opacity: 1;
      transform: translateY(0);
    }
  }

  /* Card hover effects */
  .auth-h100 {
    background-color: white;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
    transition: all 0.3s ease;
    animation: fadeInUp 0.8s ease-out forwards;
  }

  .auth-h100:hover {
    box-shadow: 0 15px 40px rgba(0, 0, 0, 0.15);
    transform: translateY(-5px);
  }

  .bg-image {
    background: linear-gradient(135deg, #1c77bd, #2196F3);
    position: relative;
    overflow: hidden;
  }

  .bg-image2::before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: url('assets/images/background5.jpg');
    background-size: cover;
    opacity: 0.06;
    animation: pulse 8s infinite alternate;
  }

  .bg-image::before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: url('assets/images/background5.jpg');
    background-size: cover;
    opacity: 0.07;
    animation: pulse 8s infinite alternate;
  }

  @keyframes pulse {
    0% {
      opacity: 0.05;
      transform: scale(1);
    }

    100% {
      opacity: 0.09;
      transform: scale(1.5);
    }
  }

  button,
  input {
    transition: all 0.3s ease;
  }

  button:hover {
    transform: translateY(-2px);
    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
  }

  button:active {
    transform: translateY(1px);
  }


  h3,
  h4 {
    font-weight: 600;
  }

  h2.text-primary,
  h4.text-primary {
    background: linear-gradient(135deg, #2196F3, #1c77bd);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
  }


  .qr-block img {
    transition: all 0.5s ease;
    animation: fadeInUp 1s ease-out forwards;
    animation-delay: 0.3s;
    opacity: 0;
  }

  .qr-block img:hover {
    transform: scale(1.03) rotate(2deg);
  }

  /* Add shine effect to the image */
  .qr-block {
    position: relative;
    overflow: hidden;
    animation: fadeInUp 0.8s ease-out forwards;
    animation-delay: 0.2s;
  }

  .qr-block::after {
    content: '';
    position: absolute;
    top: -50%;
    left: -60%;
    width: 20%;
    height: 200%;
    background: rgba(255, 255, 255, 0.3);
    transform: rotate(30deg);
    transition: all 0.7s ease;
  }

  .qr-block:hover::after {
    left: 130%;
  }

  .container-xxl {
    animation: fadeInUp 0.6s ease-out forwards;
  }

  .row.g-0.border {
    border-radius: 16px !important;
    overflow: hidden;
  }

  /* Loading animation for the system */
  @keyframes blink {

    0%,
    100% {
      opacity: 1;
    }

    50% {
      opacity: 0.5;
    }
  }
</style>

<body is="dmx-app" id="access" style="bac">
  <dmx-notifications id="notifies_access"></dmx-notifications>
  <div is="dmx-browser" id="browser_access"></div>

  <div id="timetracker-layout" class="theme-indigo">

    <div class="main p-2 py-3 p-xl-5 bg-image2">
      <div class="container-xxl">
        <div class="row align-items-center justify-content-center">
          <div class="col">
            <div class="d-flex align-item-center justify-content-center">
              <h2 class="mb-0 mt-1 mx-2 text-primary">FINAL YEAR PROJECT</h2>
            </div>
          </div>
        </div>
      </div>

      <!-- Body: Body -->
      <div class="body d-flex p-0 p-md-2 p-xl-5">
        <div class="container-xxl">
          <div class="row g-0 border border-primary rounded-3 mt-2 mt-md-0">
            <div class="col-lg-6 d-flex justify-content-center align-items-center auth-h100 bg-image py-2 py-md-0 " style="border-radius:8px 0px 0px 8px;">

              <div is="dmx-view" id="content">
                <%- await include(content, locals); %>
              </div>

            </div>

            <div class="col-lg-6 d-none d-lg-flex justify-content-center align-items-center auth-h100" style="border-radius:0px 8px 8px 0px;">
              <div class="qr-block text-center">
                <img src="assets/images/image-side.png" alt="#" class="img-fluid my-4" width="400" style="border-radius:50px;">
                <h4 class="text-primary">FileGPT!</h4>
                <p><span class="text-primary">Access your files instantly with ease!</span></p>
              </div>
            </div>
          </div>

        </div>
      </div>

    </div>

  </div>




  <script src="/bootstrap/5/js/bootstrap.bundle.min.js"></script>
  <script src="assets/js/template.js"></script>
</body>

</html>